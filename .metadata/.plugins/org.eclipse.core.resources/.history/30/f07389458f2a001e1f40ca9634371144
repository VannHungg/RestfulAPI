import jakarta.ws.rs.Produces;
import jakarta.ws.rs.core.MediaType;

@Path("v1/books")
public class BookResource {
	@GET
	@Produces(MediaType.APPLICATION_XM)
	public String sayHello() {
		return "One small meassage return from api!!";
	}
}
